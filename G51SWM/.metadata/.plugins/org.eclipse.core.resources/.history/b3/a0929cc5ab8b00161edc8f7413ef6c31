package bike;

public class MountainBike extends Bicycle {
	private boolean frontSusp;
	private boolean backSusp;

	public MountainBike(int g, int s, boolean f, boolean b) {
		super(g, s);
		frontSusp = f;
		backSusp = b;
		// TODO Auto-generated constructor stub
	}
	
	public boolean isFull(){
		if(frontSusp && backSusp){
			return true;
		} else {
			return false;
		}
	}
	
	public void printMtn(){
		
	}

}
